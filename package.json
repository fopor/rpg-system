    "build": "ng build --prod && cross-env NODE_ENV=BUILD electron-packager . --prune=true --all --ignore=build-bin --overwrite --asar --out=build-bin",
    "build-test": "cross-env NODE_ENV=BUILD electron-packager . --prune=true --ignore=build-bin --overwrite --out=build-bin --asar"
  },
  "private": true,
